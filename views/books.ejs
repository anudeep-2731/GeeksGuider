<%- include("partials/header") %>
<title><%=data["volumeInfo"].title%></title>
<div class="container pr-1 pl-1 pt-1 mt1-4 lc pb-1">
    <div class="row">
     <div class="col-lg-4 col-md-4 col-sm-4">
       <img style="height: 500px;" src="<%=data.volumeInfo.imageLinks.smallThumbnail%>">
     </div>
     <div class="col-lg-8 col-md-8 col-sm-8">
      <div class="w3-container w3-card w3-white w3-margin-bottom">
        <ul>
            <li><strong>NAME:</strong> <%=data["volumeInfo"].title%></li>
            <br>
            <li><strong>AUTHORS:</strong> <%=data["volumeInfo"].authors[0]%></li>
            <br>
            <li><strong>ISBN:</strong> <%=data["volumeInfo"]["industryIdentifiers"][0].identifier%></li>
            <br>
            <li><strong>DESCRIPTION:</strong> <%=data["volumeInfo"].description%></li>
        </ul>
      </div></div>
    </div>
</div>

<%- include("partials/footer") %>